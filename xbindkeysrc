"firefox"
   control+alt + z

"keepassx"
   Control+Alt + a

"thunar ~"
   Control+Alt + s

"brightness down ; ~/.myconfig/brightness-show.sh"
   F6

"brightness up ; ~/.myconfig/brightness-show.sh"
   F7

"volume mute ; ~/.myconfig/volume-show.sh"
   F8

"volume down ; ~/.myconfig/volume-show.sh"
   F9

"volume up ; ~/.myconfig/volume-show.sh"
   F10

"cmus-remote --prev"
    m:0x5 + c:59
    Control+Shift + comma 

"cmus-remote --next"
    m:0x5 + c:60
    Control+Shift + period 

"cmus-remote -u"
    m:0x5 + c:61
    Control+Shift + slash 

#Remark
"cmus-remote -u"
    m:0x0 + c:173
    XF86AudioPrev 

